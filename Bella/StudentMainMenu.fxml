<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.TilePane?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.Group?>
<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo.StudentMainController">
    <children>
        <VBox alignment="TOP_RIGHT">
            <Button text="PlayerView" onAction="#onPlayerViewButtonClick" fx:id="playerViewButton"/>
            <Button text="SignOut" onAction="#onSignOutButtonClick" fx:id="signOutButton"/>
        </VBox>
        <VBox alignment="CENTER">
            <Label text="Papa's Learnaria"/>
            <Button text="Play" onAction="#onPlayButtonClick" alignment="CENTER" fx:id="playButton"/>
            <Group>
                <Circle fx:id="myCircle" centerX="100.0" centerY="100.0" radius="50.0" fill="BLUE" stroke="BLACK" strokeWidth="2.0"/>
            </Group>
        </VBox>

        <TilePane alignment="CENTER">
            <children>
                <Label text="papa" alignment="BOTTOM_RIGHT"/>
                <VBox alignment="CENTER">
                    <Label text="High Score:"/>
                    <Label text="30000"/>
                </VBox>
                <HBox alignment="BOTTOM_RIGHT">
                    <Button text="setting" onAction="#onSettingsButtonClick" fx:id="settingsButton"/>
                    <Button text="stats" onAction="#onStatsButtonClick" fx:id="statsButton" />
                </HBox>
            </children>
        </TilePane>
    </children>
</VBox>





